<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Etc">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent" tools:layout_editor_absoluteY="267dp"
            tools:layout_editor_absoluteX="219dp" android:id="@+id/basicVertical">
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="66dp" android:id="@+id/buttonSection">
            <Button
                    android:text="웹 사이트"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/btnPhone" android:layout_weight="1"
                    android:onClick="onBtnUrl"/>
            <Button
                    android:text="전화걸기"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/button2" android:layout_weight="1"
                    android:onClick="onBtnPhone"/>
            <Button
                    android:text="Button"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" android:id="@+id/button3" android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="369dp" android:id="@+id/emprtSection">
        </LinearLayout>

        <Button
                android:text="돌아가기"
                android:layout_width="match_parent"
                android:layout_height="70dp" android:id="@+id/backButton"
                android:onClick="onBackButtonClick"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>